{
  "name": "masup.net.style",
  "version": "3.0.0",
  "description": "stylesheet of masuP.net",
  "main": "index.js",
  "scripts": {
    "start": "npm run css -- --watch & npm run css:dist -- --watch",
    "css": "postcss src/style.css --map -u postcss-easy-import -u postcss-preset-env -u css-mqpacker  -o build/style.css",
    "css:dist": "postcss build/style.css --no-map -u csswring -o public/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masuP9/masuP.net_style.git"
  },
  "author": "masuP9",
  "bugs": {
    "url": "https://github.com/masuP9/masuP.net_style/issues"
  },
  "homepage": "https://github.com/masuP9/masuP.net_style#readme",
  "devDependencies": {
    "css-mqpacker": "7.0.0",
    "csswring": "7.0.0",
    "postcss-cli": "6.0.1",
    "postcss-preset-env": "6.4.0",
    "postcss-easy-import": "3.0.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "normalize.css": "^8.0.0"
  },
  "browserslist": [
    "Last 1 versions",
    "Android >= 4.4",
    "IE >= 11"
  ]
}
